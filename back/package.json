    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-check": "^5.4.0"
